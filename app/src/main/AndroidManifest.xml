<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.doctorappointment">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DoctorAppointment">
        <activity
            android:name=".ContactUsActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".WelcomeScreenActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".UpdateProfileActivity"
            android:exported="false"
            android:parentActivityName=".ProfileActivity" />
        <activity
            android:name=".ContactAmbulanceActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".doctor.DoctorHomeActivity"
            android:exported="false" />
        <activity
            android:name=".doctor.AppointmentActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".doctor.PendingDoctorActivity"
            android:exported="false"
            android:parentActivityName=".AdminHomeActivity" />
        <activity
            android:name=".doctor.DoctorListActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".DoctorApplyActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".DoctorScheduleActivity"
            android:exported="false"
            android:parentActivityName=".doctor.DoctorHomeActivity" />
        <activity
            android:name=".ResetPassActivity"
            android:exported="false" />
        <activity
            android:name=".AdminHomeActivity"
            android:exported="false" />
        <activity
            android:name=".MyAppointmentActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ProfileActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".PatientLoginActivity"
            android:exported="false"/>
        <activity
            android:name=".PatientRegistrationActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="false" />
    </application>

</manifest>